ALTER TABLE CRLEMP_PROY ADD CONSTRAINT FK_CRLEMP_PROY_PROY_ID FOREIGN KEY (PROY_ID) REFERENCES CRLPROYECTO (PROY_ID)
ALTER TABLE CRLEMP_PROY ADD CONSTRAINT FK_CRLEMP_PROY_EMP_ID FOREIGN KEY (EMP_ID) REFERENCES CRLEMPLEADO (EMP_ID)
