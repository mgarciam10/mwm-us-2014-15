CREATE TABLE CRLEMPLEADO (EMP_ID NUMBER(10) NOT NULL, apellido VARCHAR2(50) NOT NULL, fechaalta DATE NULL, nombre VARCHAR2(50) NOT NULL, salario NUMBER(19) NULL, VERSION NUMBER(10) NULL, id_seguro NUMBER(10) NULL, PRIMARY KEY (EMP_ID))
CREATE TABLE CRLNUMEROTELEFONO (ID NUMBER(10) NOT NULL, numeroLocal VARCHAR2(255) NULL, prefijoInternacional VARCHAR2(255) NULL, VERSION NUMBER(10) NULL, empleado_id NUMBER(10) NULL, PRIMARY KEY (ID))
CREATE TABLE CRLSEGUROSANITARIO (ID NUMBER(10) NOT NULL, nombre VARCHAR2(150) NULL, VERSION NUMBER(10) NULL, PRIMARY KEY (ID))
CREATE TABLE CRLPROYECTO (PROY_ID NUMBER(10) NOT NULL, nombre VARCHAR2(150) NULL, VERSION NUMBER(10) NULL, PRIMARY KEY (PROY_ID))
CREATE TABLE CRLEMP_PROY (EMP_ID NUMBER(10) NOT NULL, PROY_ID NUMBER(10) NOT NULL, PRIMARY KEY (EMP_ID, PROY_ID))
ALTER TABLE CRLEMPLEADO ADD CONSTRAINT FK_CRLEMPLEADO_id_seguro FOREIGN KEY (id_seguro) REFERENCES CRLSEGUROSANITARIO (ID)
